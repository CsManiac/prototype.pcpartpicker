<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codemine.unofficial.pcpartpicker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.codemine.unofficial.pcpartpicker.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.codemine.unofficial.pcpartpicker.CPUActivity"
            android:parentActivityName="com.codemine.unofficial.pcpartpicker.MainActivity" />
        <activity
            android:name="com.codemine.unofficial.pcpartpicker.CPUCoolerActivity"
            android:parentActivityName="com.codemine.unofficial.pcpartpicker.MainActivity" />
        <activity
            android:name="com.codemine.unofficial.pcpartpicker.MotherboardActivity"
            android:parentActivityName="com.codemine.unofficial.pcpartpicker.MainActivity" />
        <activity
            android:name="com.codemine.unofficial.pcpartpicker.MemoryActivity"
            android:parentActivityName="com.codemine.unofficial.pcpartpicker.MainActivity" />
        <activity
            android:name="com.codemine.unofficial.pcpartpicker.GpuActivity"
            android:parentActivityName="com.codemine.unofficial.pcpartpicker.MainActivity"/>
        <activity
            android:name="com.codemine.unofficial.pcpartpicker.StorageActivity"
            android:parentActivityName="com.codemine.unofficial.pcpartpicker.MainActivity"/>
    </application>

</manifest>